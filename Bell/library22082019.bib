issn = {1094-4087},
journal = {Optics Express},
number = {18},
pages = {24042},
title = {{Broadband infrared spectroscopy using optical parametric oscillation in a radially-poled whispering gallery resonator}},
url = {https://www.osapublishing.org/abstract.cfm?URI=oe-23-18-24042},
volume = {23},
year = {2015}
}
@article{Lin2013,
abstract = {Optical frequency conversion processes in nonlinear materials are limited in wavelength by the accessible phase matching and the required high pump powers. In this letter, we report a novel broadband phase matching (PM) technique in high quality factor (Q) whispering gallery mode (WGM) resonators made of birefringent crystalline materials. This technique relies on two interacting WGMs, one with constant and the other with spatially oscillating phase velocity. Thus, phase matching occurs cyclically. The technique can be implemented with a WGM resonator with its disk plane parallel to the optic axis of the crystal. With a single beta barium borate (BBO) resonator in that configuration, we experimentally demonstrated efficient second harmonic generation (SHG) to harmonic wavelengths from 780 nm in the near infrared to 317 nm in the ultraviolet (UV). The observed SHG conversion efficiency is as high as 4.6{\%} (mW)-1. This broadband PM technique opens a new way for nonlinear optics applications in WGM resonators. This work is also the first reported continuous wave UV generation by direct SHG in a WGM resonator},
archivePrefix = {arXiv},
arxivId = {1303.3814},
author = {Lin, Guoping and F{\"{u}}rst, Josef U. and Strekalov, Dmitry V. and Yu, Nan},
eprint = {1303.3814},
file = {:C$\backslash$:/Users/Nadya/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Lin et al. - 2013 - Broadband second harmonic generation in whispering gallery mode resonators.pdf:pdf},
isbn = {9781457715075},
journal = {arXiv:1303.3814},
pages = {8},
title = {{Broadband second harmonic generation in whispering gallery mode resonators}},
url = {http://arxiv.org/abs/1303.3814},
year = {2013}
}
@article{Beckmann2012,
abstract = {Threshold and efficiency of optical parametric oscillation in whispering gallery resonators have been predicted to depend strongly on the coupling strength. Our experiments, varying the coupling strength continuously over 3 orders of magnitude, confirm these predictions. The pump threshold changes by a factor of 20, allowing the adaptation of the system to a wide range of input powers. The minimum threshold of 0.20 mW occurs for significant pump undercoupling. The efficiency increases monotonically with coupling, spanning 4 orders of magnitude with a maximum efficiency of 30{\%} in contact yielding output powers exceeding 1 mW.},
author = {Beckmann, Tobias and Buse, Karsten and Breunig, Ingo},
doi = {10.1364/OL.37.005250},
file = {:C$\backslash$:/Users/Nadya/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Beckmann, Buse, Breunig - 2012 - Optimizing pump threshold and conversion efficiency of whispering gallery optical parametric oscillator.pdf:pdf},
