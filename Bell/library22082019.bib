issn = {2041-1723},
journal = {Nature communications},
pages = {3109},
pmid = {24434576},
publisher = {Nature Publishing Group},
title = {{Second-harmonic generation using 4-quasi-phasematching in a GaAs whispering-gallery-mode microcavity.}},
