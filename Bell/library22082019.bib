file = {:C$\backslash$:/Users/Nadya/Downloads/Shaked{\_}2014{\_}New{\_}J.{\_}Phys.{\_}16{\_}053012.pdf:pdf},
issn = {13672630},
journal = {New Journal of Physics},
keywords = {broadband entangled photons,down conversion,nonlinear optics,quantum optics,ultrafast physics},
title = {{Observing the nonclassical nature of ultra-broadband bi-photons at ultrafast speed}},
volume = {16},
year = {2014}
}
