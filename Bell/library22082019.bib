@article{Struchalin2015,
abstract = {We report an experimental realization of adaptive Bayesian quantum state tomography for two-qubit states. Our implementation is based on the adaptive experimental design strategy proposed in [F.Husz$\backslash$'ar and N.M.T.Houlsby, Phys.Rev.A 85, 052120 (2012)] and provides an optimal measurement approach in terms of the information gain. We address the practical questions, which one faces in any experimental application: the influence of technical noise, and behavior of the tomographic algorithm for an easy to implement class of factorized measurements. In an experiment with polarization states of entangled photon pairs we observe a lower instrumental noise floor and superior reconstruction accuracy for nearly-pure states of the adaptive protocol compared to a non-adaptive. At the same time we show, that for the mixed states the restriction to factorized measurements results in no advantage for adaptive measurements, so general measurements have to be used.},
author = {Struchalin, Gleb and Pogorelov, Ivan and Straupe, Stanislav and Kravtsov, Konstantin and Radchenko, Igor and Kulik, Sergei},
journal = {ArXiv 1510.05303},
pages = {1--11},
title = {{Experimental Adaptive Quantum Tomography of Two-Qubit States}},
year = {2015}
}
@article{Shalm2013,
abstract = {Entangled quantum particles have correlations stronger than those allowed by classical physics. These correlations are the focus of of the deepest issues in quantum mechanics [1-3] and are the basis of many quantum technologies. The entanglement of discrete particle properties has been studied extensively in the context of quantum computing [4], cryptography [5], and quantum repeaters [6] while entanglement between the continuous properties of particles may play a critical role in improving the sensitivity of gravitational wave detectors [7,8], atomic clocks [9], and other high precision instruments. The attributes of three or more entangled particles are fundamentally different from those of two entangled particles [10-14]. While the discrete variables of up to 14 ions [15] and the continuous variables between three intense optical beams [16, 17] have been entangled, it has remained an open challenge to entangle the continuous properties of more than two individual particles. Here we experimentally demonstrate genuine tripartite continuous-variable entanglement between three separated particles. In our setup the three particles are photons created directly from a single input photon; the creation process leads to quantum correlations between the colours, or energies, and emission times of the photons. The entanglement between our three photons is the three-party generalization of the Einstein-Podolsky-Rosen (EPR) [1] correlations for continuous variables, and allows for new fundamental tests of quantum mechanics to be carried out. Our scheme can be extended to carry out multi-particle Franson interferometry [18,19], and opens the possibility of using additional degrees of freedom in our photons to simultaneously engineer discrete and continuous-variable hyper-entangled states that could serve as a valuable resource in a wide variety of quantum information tasks.},
archivePrefix = {arXiv},
arxivId = {1203.6315},
author = {Shalm, Lynden K and Hamel, Deny R and Yan, Zhizhong and Simon, Christoph and Resch, Kevin J and Jennewein, Thomas},
doi = {10.1038/nphys2492},
eprint = {1203.6315},
file = {:C$\backslash$:/Users/Nadya/Downloads/1203.6315.pdf:pdf},
isbn = {1745-2473},
issn = {1745-2473},
journal = {Nature Physics},
pages = {19--22},
publisher = {Nature Publishing Group},
title = {{Three-photon energy-time entanglement}},
url = {http://arxiv.org/abs/1203.6315{\%}0Ahttp://dx.doi.org/10.1038/nphys2492},
volume = {9},
year = {2013}
}
