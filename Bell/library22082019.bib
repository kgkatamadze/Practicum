author = {Rybaltovskii, A.O. and Koltashev, V.V. and Medvedkov, O.I. and Rybaltovsky, A.A. and Sokolov, V.O. and Klyamkin, S.N. and Plotnichenko, V.G. and Dianov, E.M.},
