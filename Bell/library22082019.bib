journal = {Journal of Optics},
keywords = {microresonators,non-classical light,nonlinear and quantum optics,optical wave mixing,with whispering gallery resonators},
number = {123002},
publisher = {IOP Publishing},
title = {{Nonlinear and Quantum Optics with Whispering Gallery Resonators}},
url = {http://arxiv.org/abs/1605.07972},
volume = {18},
year = {2016}
}
