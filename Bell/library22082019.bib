file = {:C$\backslash$:/Users/Nadya/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/F{\"{o}}rtsch et al. - 2013 - A versatile source of single photons for quantum information processing.pdf:pdf},
isbn = {2041-1723},
issn = {2041-1723},
journal = {Nature communications},
number = {1818},
pages = {1--2},
pmid = {23652006},
title = {{A versatile source of single photons for quantum information processing}},
url = {http://arxiv.org/abs/1204.3056{\%}5Cnhttp://www.nature.com/ncomms/journal/v4/n5/abs/ncomms2838.html},
volume = {4},
year = {2013}
