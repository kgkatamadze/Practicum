@article{Choi2008a,
abstract = {Developments in quantum information science1 rely critically on entanglement—a fundamental aspect of quantum mechanics that causes parts of a composite system to show correlations stronger than can be explained classically2 .Atomicensemblescanplaythe roleofsuchnodes4 . In particular, scalable quantum networks require the capability to create, store and distribute entan- glement among distant matter nodes by means of photonic chan- nels3 .Sofar,inthe photon-counting regime, heralded entanglement between atomic ensembleshasbeensuccessfullydemonstratedthroughprobabilistic protocols5,6 . But an inherent drawback of this approach is the com- promise between the amount of entanglement and its preparation probability, leading to intrinsically low count rates for high entan- glement.Herewereportaprotocolwhereentanglementbetweentwo atomic ensembles is created by coherent mapping of an entangled state of light. By splitting a single photon7–9 and performing sub- sequent state transfer, we separate the generation of entanglement and its storage10 ment ismapped back into photonicmodeswith overall efficiency of 17{\%}. Together with improvements in single-photon sources11 . After a programmable delay, the stored entangle- ,our protocolwill allow‘on-demand' entanglement of atomic ensembles, a powerful resource for quantuminformation science.},
author = {Choi, K S and Deng, H and Laurat, J and Kimble, H J},
journal = {Nature},
pages = {67--72},
title = {{Mapping photonic entanglement into and out of a quantum memory}},
volume = {452},
year = {2008}
}
