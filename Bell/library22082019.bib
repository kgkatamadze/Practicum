isbn = {0018-9480},
issn = {00189480},
journal = {IEEE Transactions on Microwave Theory and Techniques},
keywords = {Finite-element methods (FEMs),Microwave,Optical,Resonators,Rotational symmetry,Sapphire,Simulation,Whispering gallery modes (WGMs)},
number = {6},
