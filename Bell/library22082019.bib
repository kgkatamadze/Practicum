file = {:C$\backslash$:/Users/Nadya/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Moebius et al. - 2016 - Efficient photon triplet generation in integrated nanophotonic waveguides.pdf:pdf},
issn = {1094-4087},
journal = {Optics Express},
keywords = {Multiphoton processes,Nonlinear optics,integrated optics,parametric processes},
number = {9},
