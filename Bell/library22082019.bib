file = {:C$\backslash$:/Users/Nadya/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Rybaltovskii et al. - 2008 - Photoinduced and thermal reactions involving hydrogen in high-germania-core optical fibres.pdf:pdf},
issn = {10637818},
journal = {Quantum Electronics},
keywords = {[Germanium nanoclusters, germanosilicate fibres, M},
number = {12},
title = {{Photoinduced and thermal reactions involving hydrogen in high-germania-core optical fibres}},
volume = {38},
year = {2008}
